{
  "name": "simonpe-pulpapi",
  "version": "0.1.0",
  "author": "SimonPe",
  "summary": "Native types and providers for various pulp resources",
  "license": "GPL-3.0+",
  "source": "https://github.com/simonpe/puppet-pulpapi",
  "project_page": "https://github.com/simonpe/puppet-pulpapi",
  "issues_url": "https://github.com/simonpe/puppet-pulpapi/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.6.0 < 5.0.0"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.8.7 < 6.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "7"
      ]
    }
  ]
}
